{
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "~": "./src"
                }
            }
        ],
        [
            "module:react-native-dotenv",
            {
                "envName": "APP_ENV",
                "moduleName": "@env",
                "path": ".env"
            }
        ]
    ]
}
